programa
{
    funcao inicio()
    {
        inteiro numero, resultado

        escreva("Digite o n√∫mero para a tabuada: ")
        leia(numero)

        para (inteiro i = 1; i <= 10; i++) {
            resultado = numero * i
            escreva(numero + " x " + i + " = " + resultado + "\n")
        }
    }
}
